package day0305;

public class Ex16StringToken {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		String msg="red:pink:yellow:black:white";
		//String msg="red,pink,yellow,black,white";
		
		//|로 분리해서 출력
		String [] arr=msg.split(":");
		for(String a:arr)
			System.out.println(a);
	}

}
