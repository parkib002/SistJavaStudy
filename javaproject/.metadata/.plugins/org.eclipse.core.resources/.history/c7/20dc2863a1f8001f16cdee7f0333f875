package day0304;

class shop{
	
	private String sangpum;
	private int su;
	
	//디폴트 생성자(상품명=딸기,수량=50
	public shop() {
		sangpum="딸기";
		su=50;
	}
	//명시적 생성자
	public shop(String sangpum,int su) {
		this.sangpum=sangpum;
		this.su=su;
	}
}
