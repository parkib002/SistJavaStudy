package day0304;


class Point{
	
	int x;
	int y;
	
	//디폴트 생성자
	public Point() {
		System.out.println("Super_디폴트 생성자 호출");
	}
	//명시적 생성자
	public Point(int x,int y) {
		System.out.println("Super_인자값 있는 명시적 생성자 호출");
		this.x=x;
		this.y=y;
	}
}
/////////////////////
public class Ex3SuperSub {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
