package day0305;

class OuterObj{
	
	
	class InnerObj{
		
		public void write()
		{
			System.out.println("내부클래스의 메서드");
		}
	}
	
	public void show()
	{
		System.out.println("외부클래스의 메서드");
	}
	
}
