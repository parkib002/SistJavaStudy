			url: '/comm/kweather/currentWeather2.json',
			dataType: 'json',
			contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
			success: function(data) {
				var resultToday = data.applyDate_YYYY + "." + data.applyDate_MM + "." + data.applyDate_DD;
				var resultTemp = data.temp+"<em>C</em>";
				switch(data.icon) {
					case '1':
						$('.ico-weather').addClass('sunny');
						$('.ico-weather-blind').text('맑음');
						break;
					case '2':
					case '3':
						$('.ico-weather').addClass('cloudy');
						$('.ico-weather-blind').text('흐림');
						break;
					case '5':
					case '6':
						$('.ico-weather').addClass('cloudy');
						$('.ico-weather-blind').text('흐림');
						break;
					case '4':
						$('.ico-weather').addClass('cloudy');
						$('.ico-weather-blind').text('흐림');
						break;
					case '7':
					case '8':
					case '9':
						$('.ico-weather').addClass('storm');
						$('.ico-weather-blind').text('뇌우');
						break;
					case '10':
					case '11':
					case '12':
					case '13':
					case '14':
					case '15':
					case '16':
					case '17':
						$('.ico-weather').addClass('littleRain');
