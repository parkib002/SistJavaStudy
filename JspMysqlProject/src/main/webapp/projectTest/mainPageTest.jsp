					case '36':
					case '37':
					case '38':
						$('.ico-weather').addClass('snow');
						$('.ico-weather-blind').text('눈');
						break;
					case '39':
					case '40':
						$('.ico-weather').addClass('cloudy');
						$('.ico-weather-blind').text('흐림');
						break;
					default :

				}
				$(".weather-cont .degree").html(resultTemp);
			},
			error: function(data, status, err) {
				console.log('error forward weather : ' + JSON.stringify(data));
			}
		});
	}

	function getSearchRankings() {

		$.ajax({
			type : "post",
			url: '/comm/api/search/rankings/ko',
			dataType: 'json',
			success: function(data) {
				let result = data.result;
				if(result.code == 0){
					var col1html = '';
					var col2html = '';
					$.each(result.rankings, function(i, item){
						if(i<5) {
							col1html += '<li>';
							col1html += '    <span class="num">'+(i+1)+'</span>';
							col1html += '    <a href="/search?currentPage=1&search_radio=K&searchTerm='+item.KEYWORD+'&searchTy=&ctgrySn=&tursmAreaCode=">'+item.KEYWORD+'</a>'
							col1html += '</li>\n';
						}else{
							col2html += '<li>';
							col2html += '    <span class="num">'+(i+1)+'</span>';
							col2html += '    <a href="/search?currentPage=1&search_radio=K&searchTerm='+item.KEYWORD+'&searchTy=&ctgrySn=&tursmAreaCode=">'+item.KEYWORD+'</a>'
							col2html += '</li>\n';
						}
					});
					$('.uiSearchRankingCol1').html(col1html);
					$('.uiSearchRankingCol2').html(col2html);
				}

			},
			error: function(data, status, err) {
				console.log('error forward search ranking : ' + JSON.stringify(data));
			}
		});

	}

